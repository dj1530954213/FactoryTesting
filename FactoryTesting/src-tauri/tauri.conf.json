{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "factory-testing",
  "version": "0.1.0",
  "identifier": "com.factory.testing",
  "build": {
    "frontendDist": "../dist/factory-testing/browser",
    "devUrl": "http://localhost:4200",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "factory-testing",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": false
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
